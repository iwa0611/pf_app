<h1>管理画面</h1>
<% @users.each do |user| %>
  <ul>
    <li>
      <div>ID:<%= user.id %></div>
    </li>
    <li>
      <div>NAME:<%= user.name %></div>
    </li>
    <li>
      <%= link_to "アカウント削除", registration_path(resource_name), data: { confirm: "削除しますか?" }, method: :delete %>
    </li>
  </ul>
<% end %>
